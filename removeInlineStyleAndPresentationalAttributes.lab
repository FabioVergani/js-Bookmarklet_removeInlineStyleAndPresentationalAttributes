
javascript:void((function(win){
	var e,k,w=win,d=w.document,
	m=d.getElementsByTagName('*'),
	i=m.length+0,
	p=['style','align','background','bgcolor','border','cellpadding','cellspacing','color','face','height','hspace','marginheight','marginwidth','noshade','nowrap','valign','vspace','width','vlink','alink','text','link','frame','frameborder','clear','scrolling'],
	j=p.length+0;
	while(i--!==0){e=m[i];k=j+0;while(k--!==0){e.removeAttribute(p[k]);};};

	e=w.livesource;
	if(!e||e.closed){w.livesource=e=w.open();};
	j=e.document;
	j.title=w.location.pathname.split('/').pop();
	k=j.body;
	k.setAttribute('style','font:1.2em/normal monospace;white-space:pre-wrap;background:#F8F8F8;color:#000;');
	k.setAttribute('spellcheck','false');
	k.setAttribute('contenteditable','true');

	k.appendChild(d.createTextNode(d.documentElement.outerHTML));
	e.ondblclick=function(){e.getSelection().selectAllChildren(k);};
	e.focus();
})(window));




